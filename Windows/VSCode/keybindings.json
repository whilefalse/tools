// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+shift+b",
        "command": "workbench.action.tasks.runTask"
    },
    {
        "key": "ctrl+oem_comma",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    // Visual block mode
    {
        "key": "ctrl+shift+V",
        "command": "extension.vim_ctrl+v"
    }
]